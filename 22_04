#include <stdio.h>
#include <stdlib.h>
#include <time.h>

#define SIZE 5

int main() {
    srand(time(0)); 
    int matriz[SIZE][SIZE];


    for (int i = 0; i < SIZE; i++) {
        for (int j = 0; j < SIZE; j++) {
            matriz[i][j] = rand() % 101;
        }
    }


    for (int i = 0; i < SIZE; i++) {
        for (int j = 0; j < SIZE; j++) {
            printf("%3d ", matriz[i][j]);
        }
        printf("\n");
    }

    return 0;
}





atividade 2



#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int main() {
    srand(time(NULL));
    int cmp = 25;
    int vetor[cmp];
    int i;

    for (i = 0; i < cmp; i++) {
        vetor[i] = rand() % 101;
    }

    //LaÃ§o para mostrar o vetor
    for (i = 0; i < cmp; i++) {
        printf("%d\n", vetor[i]);
    }
    return 0;
}
